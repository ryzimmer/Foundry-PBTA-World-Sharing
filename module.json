{
  "id": "my-shared-compendia",
  "title": "My Shared Compendia",
  "description": "Creates default set of compendia that are shared across Foundry VTT worlds hosted on the same server. Forked to work for any system",
  "authors": [
    {
      "name": "Stephan Sch√∂lzel"
    },
    {
      "name": "Napoleone Piani"
    }
  ],
  "version": "1",
  "compatibility": {
    "minimum": "10",
    "verified": "10"
  },
  "packs": [
    {
      "name": "actors",
      "label": "Actors (shared)",
      "path": "./packs/actors.db",
      "module": "my-shared-compendia",
      "type": "Actor"
    },
    {
      "name": "items",
      "label": "Items (shared)",
      "path": "./packs/items.db",
      "module": "my-shared-compendia",
      "type": "Item"
    },
    {
      "name": "scenes",
      "label": "Scenes (shared)",
      "path": "./packs/scenes.db",
      "module": "my-shared-compendia",
      "type": "Scene"
    },
    {
      "name": "JournalEntries",
      "label": "Journal Entries (shared)",
      "path": "./packs/journalentry.db",
      "module": "my-shared-compendia",
      "type": "JournalEntry"
    },
    {
      "name": "macros",
      "label": "Macros (shared)",
      "path": "./packs/macro.db",
      "module": "my-shared-compendia",
      "type": "Macro"
    },
    {
      "name": "rolltables",
      "label": "Roll Tables (shared)",
      "path": "./packs/rolltables.db",
      "module": "my-shared-compendia",
      "type": "RollTable"
    },
    {
      "name": "playlist",
      "label": "Playlist (shared)",
      "path": "./packs/playlist.db",
      "module": "my-shared-compendia",
      "type": "Playlist"
    }
  ],
  "download": "https://github.com/ryzimmer/My-Shared-Compendia/archive/refs/heads/main.zip",
  "manifest": "https://raw.githubusercontent.com/ryzimmer/My-Shared-Compendia/main/module.json",
  "url": "https://github.com/ryzimmer/My-Shared-Compendia"
}
